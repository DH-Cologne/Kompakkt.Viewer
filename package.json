{
  "name": "kompakkt",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "local": "ng serve --host=0.0.0.0",
    "build": "ng build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "ng lint",
    "report": "jscpd src/",
    "prettier": "prettier 'src/**/*.ts' --write",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "e2e": "concurrently \"npm run start\" \"wait-on http-get://localhost:4200 && npm run cy:run\" --kill-others --success first",
    "e2e-gui": "concurrently \"npm run start\" \"wait-on http-get://localhost:4200 && npm run cy:open\" --kill-others --success first"
  },
  "contributors": [
    {
      "name": "Zoe Schubert"
    },
    {
      "name": "Kai Niebes"
    },
    {
      "name": "Jan G. Wieners",
      "email": "jan.wieners@uni-koeln.de",
      "url": "http://www.jan-wieners.de"
    }
  ],
  "private": true,
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/cypress/",
      "<rootDir>/test/e2e/"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "jest-preset-angular/build/InlineFilesTransformer",
          "jest-preset-angular/build/StripStylesTransformer"
        ]
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@angular/animations": "^9.0.6",
    "@angular/cdk": "^9.1.3",
    "@angular/common": "^9.0.6",
    "@angular/compiler": "^9.0.6",
    "@angular/core": "^9.0.6",
    "@angular/forms": "^9.0.6",
    "@angular/material": "^9.1.3",
    "@angular/platform-browser": "^9.0.6",
    "@angular/platform-browser-dynamic": "^9.0.6",
    "@angular/router": "^9.0.6",
    "@kompakkt/shared": "Kompakkt/Shared",
    "@types/pouchdb-upsert": "^2.2.6",
    "babylonjs": "^4.2.0",
    "babylonjs-gui": "^4.2.0",
    "babylonjs-loaders": "^4.2.0",
    "core-js": "^3.6.4",
    "file-saver": "^2.0.2",
    "fscreen": "^1.0.2",
    "marked": "^0.8.0",
    "ngx-color": "^5.0.1",
    "ngx-markdown": "^9.0.0",
    "ngx-socket-io": "^3.0.1",
    "pepjs": "^0.5.2",
    "pouchdb": "^7.2.1",
    "pouchdb-find": "^7.2.1",
    "pouchdb-upsert": "^2.2.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.6",
    "@angular/cli": "9.0.6",
    "@angular/compiler-cli": "^9.0.6",
    "@angular/language-service": "^9.0.6",
    "@types/file-saver": "^2.0.1",
    "@types/fscreen": "^1.0.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.1",
    "@types/pouchdb": "^6.4.0",
    "codelyzer": "^5.2.1",
    "concurrently": "^5.1.0",
    "cypress": "^4.2.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-preset-angular": "^8.1.2",
    "jscpd": "^3.1.0",
    "jscpd-badge-reporter": "^1.1.3",
    "jscpd-html-reporter": "^2.0.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "ts-node": "~8.6.2",
    "tslint": "~6.1.0",
    "typescript": "^3.8.3",
    "wait-on": "^4.0.1"
  },
  "jscpd": {
    "reporters": [
      "html",
      "badge"
    ],
    "ignore": [
      "node_modules"
    ],
    "gitignore": true
  }
}

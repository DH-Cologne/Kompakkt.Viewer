{
  "name": "kompakkt",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "start": "ng serve --prod",
    "local": "ng serve --host=0.0.0.0",
    "build": "ng build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "report": "jscpd src/",
    "prettier": "prettier 'src/**/*.ts' --write"
  },
  "contributors": [
    {
      "name": "Zoe Schubert"
    },
    {
      "name": "Kai Niebes"
    },
    {
      "name": "Jan G. Wieners",
      "email": "jan.wieners@uni-koeln.de",
      "url": "http://www.jan-wieners.de"
    }
  ],
  "private": true,
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/cypress/",
      "<rootDir>/test/e2e/"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "jest-preset-angular/build/InlineFilesTransformer",
          "jest-preset-angular/build/StripStylesTransformer"
        ]
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@angular/animations": "^9.0.2",
    "@angular/cdk": "^9.0.1",
    "@angular/common": "^9.0.2",
    "@angular/compiler": "^9.0.2",
    "@angular/core": "^9.0.2",
    "@angular/forms": "^9.0.2",
    "@angular/material": "^9.0.1",
    "@angular/platform-browser": "^9.0.2",
    "@angular/platform-browser-dynamic": "^9.0.2",
    "@angular/router": "^9.0.2",
    "@types/pouchdb-upsert": "^2.2.6",
    "babylonjs": "^4.0.3",
    "babylonjs-gui": "^4.0.3",
    "babylonjs-loaders": "^4.0.3",
    "core-js": "^3.1.3",
    "file-saver": "^2.0.2",
    "marked": "^0.8.0",
    "ngx-color": "^4.1.1",
    "ngx-markdown": "^9.0.0",
    "ngx-socket-io": "^3.0.1",
    "pepjs": "^0.5.2",
    "pouchdb": "^7.2.1",
    "pouchdb-find": "^7.2.1",
    "pouchdb-upsert": "^2.2.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.3",
    "@angular/cli": "9.0.3",
    "@angular/compiler-cli": "^9.0.2",
    "@angular/language-service": "^9.0.2",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^25.1.3",
    "@types/node": "^12.11.1",
    "@types/pouchdb": "^6.4.0",
    "codelyzer": "^5.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-preset-angular": "^8.0.0",
    "jscpd": "^2.0.16",
    "jscpd-badge-reporter": "^1.1.3",
    "jscpd-html-reporter": "^2.0.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.2",
    "ts-node": "~8.2.0",
    "tslint": "~5.16.0",
    "typescript": "^3.6.4"
  },
  "jscpd": {
    "reporters": [
      "html",
      "badge"
    ],
    "ignore": [
      "node_modules"
    ],
    "gitignore": true
  }
}
